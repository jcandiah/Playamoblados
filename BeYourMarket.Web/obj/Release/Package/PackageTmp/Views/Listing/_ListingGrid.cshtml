@model BeYourMarket.Web.Models.SearchListingModel

@{
    Layout = null;
}

<div class="row-eq-height">
    @foreach (var item in Model.ListingsPageList)
    {
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
            @Html.Partial("~/Views/Listing/_ListingCard.cshtml", item)
        </div>
    }       
</div>
@if (Model.ListingsPageList2.Count > 0)
{
    <br />
    <div class="well w-lg" style="width: 100%">No encontramos una propiedad acorde a las fechas ingresadas, pero tenemos alternativas que varian de uno a 2 dias.</div>
    <div class="row-eq-height">
        @foreach (var item2 in Model.ListingsPageList2)
        {
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
                @Html.Partial("~/Views/Listing/_ListingCard.cshtml", item2)
            </div>
        }
    </div>
}

